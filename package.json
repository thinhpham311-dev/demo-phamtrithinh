    "axios": "^0.27.2",
    "classnames": "^2.2.6",
    "dayjs": "^1.11.10",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "history": "^5.3.0",
    "i18next": "^21.6.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^18.2.0",
    "react-html-parser": "^2.0.2",
    "react-i18next": "^11.15.1",
    "react-icons": "^4.4.0",
    "react-modal": "^3.13.1",
    "react-number-format": "^4.9.1",
    "react-popper": "^2.3.0",
    "react-portal": "^4.2.2",
    "react-quill": "^2.0.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.1",
    "react-scroll": "^1.8.3",
    "react-tooltip": "^4.2.21",
    "react-window": "^1.8.6",
    "redux": "^4.1.1",
    "redux-persist": "^6.0.0",
    "twin.macro": "^3.0.0-rc.1",
    "validator": "^13.11.0",
    "yup": "^0.32.10"
  },
  "scripts": {
    "start": "run-p watch:css react-scripts:start",
    "build": "run-s build:css react-scripts:build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "cross-env TAILWIND_MODE=build NODE_ENV=production postcss src/assets/styles/app.css -o src/index.css",
    "watch:css": "cross-env TAILWIND_MODE=watch NODE_ENV=development postcss src/assets/styles/app.css -o src/index.css --watch",
    "react-scripts:start": "delay 5 && react-scripts start",
    "react-scripts:build": "react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "delay-cli": "^1.1.0",
    "miragejs": "^0.1.43",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "7.x",
    "tailwindcss": "^3.0.7"
  }
}
